<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<!-- Servlet Configuration -->
	<servlet>
		<servlet-name>html</servlet-name>
		<servlet-class>com.nit.servlet.HtmlServlet</servlet-class>
		<load-on-startup>10</load-on-startup>
	</servlet>
	<!-- Servlet Mapping/linking with URL -->
	<servlet-mapping>
		<servlet-name>html</servlet-name>
		<url-pattern>/htmlurl</url-pattern>
	</servlet-mapping>

	<!-- Servlet Configuration -->
	<servlet>
		<servlet-name>excel</servlet-name>
		<servlet-class>com.nit.servlet.ExcelServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<!-- Servlet Mapping/linking with URL -->
	<servlet-mapping>
		<servlet-name>excel</servlet-name>
		<url-pattern>/excelurl</url-pattern>
	</servlet-mapping>

	<!-- Servlet Configuration -->
	<servlet>
		<servlet-name>word</servlet-name>
		<servlet-class>com.nit.servlet.WordServlet</servlet-class>
		<load-on-startup>5</load-on-startup>
	</servlet>
	<!-- Servlet Mapping/linking with URL -->
	<servlet-mapping>
		<servlet-name>word</servlet-name>
		<url-pattern>/wordurl</url-pattern>
	</servlet-mapping>

	<!-- Servlet Configuration -->
	<servlet>
		<servlet-name>plain</servlet-name>
		<servlet-class>com.nit.servlet.PlainServlet</servlet-class>
		<load-on-startup>-1</load-on-startup>
	</servlet>
	<!-- Servlet Mapping/linking with URL -->
	<servlet-mapping>
		<servlet-name>plain</servlet-name>
		<url-pattern>/plainurl</url-pattern>
	</servlet-mapping>

	<!-- Servlet Configuration -->
	<servlet>
		<servlet-name>xml</servlet-name>
		<servlet-class>com.nit.servlet.XmlServlet</servlet-class>
		<load-on-startup>20</load-on-startup>
	</servlet>
	<!-- Servlet Mapping/linking with URL -->
	<servlet-mapping>
		<servlet-name>xml</servlet-name>
		<url-pattern>/xmlurl</url-pattern>
	</servlet-mapping>

	<!-- Servlet Configuration -->
	<servlet>
		<servlet-name>lc</servlet-name>
		<servlet-class>com.nit.servlet.LcTestServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<!-- Servlet Mapping/linking with URL -->
	<servlet-mapping>
		<servlet-name>lc</servlet-name>
		<url-pattern>/lcurl</url-pattern>
	</servlet-mapping>
</web-app>