<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>EmployeeOfficeDBApp-initParams</display-name>
	<welcome-file-list>
		<welcome-file>search.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>emp</servlet-name>
		<servlet-class>com.nit.servlet.EmployeeSearchServlet</servlet-class>
		<init-param>
			<param-name>driverClass</param-name>
			<param-value>oracle.jdbc.driver.OracleDriver</param-value>
		</init-param>
		<init-param>
			<param-name>dbUrl</param-name>
			<param-value>jdbc:oracle:thin:@localhost:1521:xe</param-value>
		</init-param>
		<init-param>
			<param-name>dbUsername</param-name>
			<param-value>system</param-value>
		</init-param>
		<init-param>
			<param-name>dbPassword</param-name>
			<param-value>manager</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>emp</servlet-name>
		<url-pattern>/empurl</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>emptest</servlet-name>
		<servlet-class>com.nit.servlet.EmployeeTestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>emptest</servlet-name>
		<url-pattern>/emptesturl</url-pattern>
	</servlet-mapping>
</web-app>